#use with limit
--disable_warnings
drop database if exists with_limit;
--enable_warnings
create database with_limit;
use with_limit;
with recursive CTETEMP(N) as
(
SELECT 1
UNION ALL
SELECT N+1 FROM CTETEMP WHERE N < 20
)
SELECT N as order_id
From CTETEMP
limit 10;

with recursive CTETEMP(N) as
(
SELECT 1
UNION ALL
SELECT N+1 FROM CTETEMP WHERE N < 20
)
SELECT N as order_id
From CTETEMP;
drop database with_limit;

